{
    "extends": "@onlook/typescript/next-react.json",
    "compilerOptions": {
        "baseUrl": ".",
        "allowArbitraryExtensions": true,
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "/*": [
                "./*"
            ],
            "~/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        ".eslintrc.cjs",
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/*.cjs",
        "**/*.js",
        "src",
        "test",
        ".next/types/**/*.ts",
        "messages/en.d.json.ts",
        "next.config.ts"
    ],
    "exclude": [
        "node_modules",
        ".next",
        "public/onlook-preload-script.js"
    ]
}